<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<StackPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <BorderPane fx:id="pane" prefHeight="200.0" prefWidth="200.0">
      <bottom>
         <HBox alignment="CENTER_RIGHT" spacing="10.0" BorderPane.alignment="CENTER">
            <Button fx:id="accept" defaultButton="true" mnemonicParsing="false" text="Accept"/>
            <Button fx:id="cancel" cancelButton="true" mnemonicParsing="false" text="Cancel"/>
         </HBox>
      </bottom>
      <center>
         <Text fx:id="text" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="200.13671875" BorderPane.alignment="CENTER"/>
      </center>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
      <effect>
         <DropShadow/>
      </effect>
   </BorderPane>
</StackPane>
