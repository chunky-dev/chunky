<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.10-internal" xmlns:fx="http://javafx.com/fxml/1">
  <top>
    <Label text="A warning occurred!&#10;&#10;Below is more information.&#10;">
      <BorderPane.margin>
        <Insets left="10.0" right="10.0" top="10.0" />
      </BorderPane.margin>
    </Label>
  </top>
  <center>
    <TabPane fx:id="tabPane" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" />
  </center>
  <bottom>
    <HBox alignment="CENTER_RIGHT" spacing="10.0" BorderPane.alignment="CENTER">
      <Button mnemonicParsing="false" onAction="#dismissMessage" text="Dismiss" />
      <Button mnemonicParsing="false" onAction="#dismissAllMessages" text="Dismiss All" />
      <BorderPane.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" />
      </BorderPane.margin>
    </HBox>
  </bottom>
</BorderPane>
